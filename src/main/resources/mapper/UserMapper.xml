<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.upimage.upimg.mapper.UserMapper">
    <select id="findAll" resultType="com.upimage.upimg.entity.User">
        SELECT * FROM user
    </select>

    <!--    æ·»åŠ -->
    <insert id="userinsert" parameterType="com.upimage.upimg.entity.User">
        insert into user(user_id,user_head,user_name,age,sex,created_at,updated_at,pass_word) value (#{user_id},#{user_head},#{user_name},#{age},#{sex},#{created_at},#{updated_at},#{pass_word});
    </insert>

</mapper>